.thumbnail:hover { 
transform: scale(0.9); 
}


*{
	box-sizing:border-box;
	margin:0;
	padding:0;	
}

.slider{
	position:relative;
	width:60%;
	overflow:hidden;
	right:-300px;
}
.images{
	display:flex;
	width:100%;
}
.images img{
	height:400px;
	width:100%;
	transition:all 0.15s ease;
}
.images input{
	display:none;
}
.dots{
	display:flex;
	justify-content:center;
	margin:5px;
}
.dots label{
	height:20px;
	width:20px;
	border-radius:50%;
	border:solid #80225f 3px;
	cursor:pointer;
	transition:all 0.15s ease;
	margin:5px;
}
.dots label:hover{background:#80225f};
#img1:checked ~ .m1{
	margin-left:0;
}
#img2:checked ~ .m2{
	margin-left:-80%;
}
#img3:checked ~ .m3{
	margin-left:-100%
}
#img3:checked ~ .m3{
	margin-left:-250%;
}

.container{
        width: 200px;
        height: 200px;
        position: relative;
        margin:-10px;
    }
    .box{
        width:740%;
        height: 150%;            
        position: absolute;
        top: 0;
        left: 0;
		box-sizing:border-box;
        /* for demo purpose  */
    }
    .stack-top{
        z-index: 9;
        position:relative;
		top:-50px;
		margin-left:200px;
		
    }
	
#grad{background-image:linear-gradient(to right,#80225f ,#415364,#52c0cf );}
#grad1{background-image:linear-gradient(to right,#415364,#acdedd );}
#grad2{background-image:linear-gradient(to right,#80225f ,#415364,#80225f );}
.border{border:2px solid #80225f;}


